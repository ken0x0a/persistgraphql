{
  "name": "@ken0x0a/persistgraphql",
  "version": "0.3.11",
  "description": "A build tool for GraphQL projects.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "persistgraphql": "./bin/persistgraphql"
  },
  "files": [
    "lib/**",
    "src/**",
    "bin/*"
  ],
  "scripts": {
    "pretest": "npm run compile",
    "test": "mocha --reporter spec --full-trace lib/test/tests.js",
    "posttest": "npm run lint",
    "compile": "tsc",
    "build.ts": "git clean -nx lib && tsc -p tsconfig.build.json",
    "build": "git clean -nx lib && ncc build src/main.ts -o lib -m",
    "start": "git clean -nx lib && ncc build src/main.ts -w -o lib -m",
    "lint": "tslint -c tslint.json test/**/*.ts; tslint src/**/*.ts"
  },
  "dependencies": {
    "apollo-client": "^1.1",
    "graphql": ">=0.9.4 <0.11",
    "graphql-tag": "^2.0.0",
    "lodash": "^4.17.11",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.119",
    "@types/node": "^7.0.14",
    "@types/yargs": "^6.6.0",
    "fetch-mock": "^5.10.0",
    "source-map-support": "^0.4.15",
    "tslint": "^5.1.0",
    "typescript": "^2.3.1"
  },
  "optionalDependencies": {
    "@types/graphql": "^0.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ken0x0a/persistgraphql.git"
  },
  "author": "Dhaivat Pandya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ken0x0a/persistgraphql/issues"
  },
  "homepage": "https://github.com/ken0x0a/persistgraphql#readme"
}
